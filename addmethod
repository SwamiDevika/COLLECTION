import java.util.ArrayList;
//add() method to Add elements
public class Main { 
  public static void main(String[] args) { 
    ArrayList<String> cars = new ArrayList<String>();
    cars.add("Volvo");
    cars.add("BMW");
    cars.add("Ford");
    cars.add("Mazda");
    for (String i : cars) {
      System.out.println(i);
      }
      
    
  } 
}
/*
Volvo
BMW
Ford
Mazda
*/
